RewriteEngine On
Options All -Indexes
RewriteBase /

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d


# RewriteRule ^(.*)$ http://127.0.0.1/f1/controller/round.php [L,R=301,NC]

DirectoryIndex controller/round.php

RewriteRule ^index\.php$ f1/controller/round.php [L,R=301]
RewriteRule ^round\.php$ f1/controller/round.php [L,R=301]
RewriteRule ^guess\.php$ f1/controller/guess.php [L,R=301]
RewriteRule ^pitTime\.php$ f1/controller/guess.php [L,R=301]
RewriteRule ^pilot_details\.php$ f1/controller/pilot_details.php [L,R=301]